@model IEnumerable<上海燕洵物联网科技有限公司人事管理系统.Models.EmpViewModel>

@{
    ViewBag.Title = "GetAllEmp";
}

<h2>GetAllEmp</h2>

<p>
    @Html.ActionLink("新增员工", "AddEmp")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Ename)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Esex)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Papersnum)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Ephone)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Eduty)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Email)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Tracttype)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Etype)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ERemark)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Ename)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Esex)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Papersnum)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Ephone)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Eduty)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Email)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Tracttype)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Etype)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ERemark)
        </td>
        <td>
            @Html.ActionLink("修改", "Edit", new { id=item.Id }) |
            @Html.ActionLink("详情", "Details", new { id=item.Id }) |
            @Html.ActionLink("删除", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
